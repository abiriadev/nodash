{
	"name": "nodash",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev:backend": "pnpm --filter @nodash/backend dev",
		"build:backend": "pnpm --filter @nodash/backend build",
		"build:frontend": "pnpm --filter @nodash/frontend build",
		"build": "pnpm -r build",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@eslint/js": "^10.0.1",
		"eslint": "^10.0.0",
		"eslint-plugin-react": "^7.37.5",
		"globals": "^17.3.0",
		"jiti": "^2.6.1",
		"prettier": "^3.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0"
	}
}
