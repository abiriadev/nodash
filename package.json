{
	"name": "nodash",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev:backend": "pnpm --filter @nodash/backend dev",
		"build:backend": "pnpm --filter @nodash/backend build",
		"build:frontend": "pnpm --filter @nodash/frontend build",
		"build": "pnpm -r build",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"prettier": "^3.7.4",
		"typescript": "^5.9.3"
	}
}
