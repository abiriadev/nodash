{
	"name": "@nodash/backend",
	"version": "1.0.0",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"build": "swc src -d dist --strip-leading-paths",
		"build:type": "tsc",
		"start": "node --enable-source-maps --env-file .env dist/index.js",
		"dev": "tsx --env-file .env ./src/index.ts",
		"dev:wrangler": "wrangler dev",
		"test": "vitest run",
		"deploy": "wrangler deploy"
	},
	"dependencies": {
		"@hono/node-server": "^1.19.9",
		"@hono/standard-validator": "^0.2.2",
		"@hono/swagger-ui": "^0.5.3",
		"@hono/zod-validator": "^0.7.6",
		"better-sqlite3": "^12.6.0",
		"es-toolkit": "^1.43.0",
		"hono": "^4.11.4",
		"hono-openapi": "^1.1.2",
		"uuid": "^13.0.0",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@swc/cli": "^0.7.10",
		"@swc/core": "^1.15.8",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^25.0.8",
		"@types/uuid": "^11.0.0",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite-tsconfig-paths": "^6.0.4",
		"vitest": "^4.0.17",
		"wrangler": "^4.59.2"
	}
}
