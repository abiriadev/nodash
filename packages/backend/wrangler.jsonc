{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "nodash",
	"main": "src/worker.ts",
	"compatibility_date": "2026-01-16",
	"vars": {
		"VERSION": "1.0.0",
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "nodash-db",
			"database_id": "REPLACE_WITH_YOUR_D1_DATABASE_ID",
		},
	],
	"observability": {
		"enabled": true,
		"head_sampling_rate": 1,
	},
}
